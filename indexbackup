  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <script src="jquery.csv.js"></script>
  <script>
    window.onload = function() {
      var itemsLeft = document.getElementsByClassName("technologyInfo");
      for (var i = 0; i < itemsLeft.length; i++) {
        var iconTitleHTML = itemsLeft[i].innerHTML;
        var newIconTitleDiv = `<div style="display: grid !important; grid-template-rows: 1fr; grid-template-columns: 50px 1fr; justify-items: left;">` + iconTitleHTML + `</div`;
          itemsLeft[i].innerHTML = newIconTitleDiv;
          var elem = document.createElement("div");
          elem.class = "costDivItm";
          elem.style = "display: grid; grid-template-columns: repeat(4, 1fr);";
          elem.innerHTML = `
          <label for="curLevelInput" style="display: flex; align-items: center;">Current level:</label>
          <input type="text" id="curLevelInput_`+i +`" class="levelInput" name="curLevelInput" style="width:20px;" value="0">
          <label for="targetLevelInput" style="display: flex; align-items: center;">Target level:</label>
          <input type="text" id="targetLevelInput_` +i + `" class="levelInput" name="targetLevelInput" style="width:20px;" value="0">`;
          itemsLeft[i].appendChild(elem);
        }

        var itemsRight = document.getElementsByClassName("technologyRow");
        for (var i = 0; i < itemsRight.length; i++) {
          itemsRight[i].innerHTML += `
          <div class="technologyShort" style="display: block !important;">
            <div class="costDivItem" style="grid-template-columns: repeat(3, 1fr);">
              <div class="resDiv">
                <div class="resourceIcon metal"></div>
                <div class="resVal" id="resMetal_`+ i +`">0</div>
              </div>
              <div class="resDiv">
                <div class="resourceIcon crystal"></div>
                <div class="resVal" id="resCrystal_`+ i +`">0</div>
              </div>
              <div class="resDiv">
                <div class="resourceIcon deuterium"></div>
                <div class="resVal" id="resDeut_`+ i +`">0</div>
              </div>
            </div></div>`;
          }

          $('input:text').click(
              function(){
                  $(this).val('');
              });


              $.fn.digits = function(){
                  return this.each(function(){
                      $(this).text( $(this).text().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1.") );
                  })
              }

          $(function(){
            var data;
                $.ajax({
                  type: "GET",
                  url: "LFValues.csv",
                  dataType: "text",
                  success: function(response)
                  {
                    data = $.csv.toArrays(response);
                    $('.levelInput').keyup(function() {
                      var itemNumber = parseInt($(this).attr("id").split("_")[1]);
                      var currentLevelInputId = '#curLevelInput_' + itemNumber;
                      var targetLevelInputId = '#targetLevelInput_' + itemNumber;
                      var currentLevelValue = parseInt($(currentLevelInputId).val());
                      var targetLevelValue = parseInt($(targetLevelInputId).val());
                      var metalTotalCost = 0;
                      var crystalTotalCost = 0;
                      var deutTotalCost = 0;

                      var metalBaseCost = parseInt(data[itemNumber][0]);
                      var crystalBaseCost = parseInt(data[itemNumber][1]);
                      var deutBaseCost = parseInt(data[itemNumber][2]);
                      var metalInc = parseFloat(data[itemNumber][3]);
                      var crystalInc = parseFloat(data[itemNumber][4]);
                      var deutInc = parseFloat(data[itemNumber][5]);

                      if (targetLevelValue < 250 && currentLevelValue < targetLevelValue) {
                      for (var i = currentLevelValue+1; i <= targetLevelValue; i++) {
                        var metalCurrentCost = parseInt(Math.floor(metalBaseCost * i * metalInc**(i-1))) ? parseInt(Math.floor(metalBaseCost * i * metalInc**(i-1))) : 0;
                        var crystalCurrentCost = parseInt(Math.floor(crystalBaseCost * i * crystalInc**(i-1))) ? parseInt(Math.floor(crystalBaseCost * i * crystalInc**(i-1))) : 0;
                        var deutCurrentCost = parseInt(Math.floor(deutBaseCost * i * deutInc**(i-1))) ? parseInt(Math.floor(deutBaseCost * i * deutInc**(i-1))) : 0;
                        metalTotalCost += metalCurrentCost;
                        crystalTotalCost += crystalCurrentCost;
                        deutTotalCost += deutCurrentCost;
                      }

                      if (metalBaseCost == 0 || Number.isSafeInteger(metalTotalCost)) {
                        $('#resMetal_' + itemNumber.toString()).text(metalTotalCost).digits();
                      }
                      else {
                        $('#resMetal_' + itemNumber.toString()).text('bruh pls.')
                      }

                      if (crystalBaseCost == 0 || Number.isSafeInteger(crystalTotalCost)) {
                        $('#resCrystal_' + itemNumber.toString()).text(crystalTotalCost).digits();
                      }
                      else {
                        $('#resCrystal_' + itemNumber.toString()).text('bruh pls.')
                      }

                      if (deutBaseCost == 0 || Number.isSafeInteger(deutTotalCost)) {
                        $('#resDeut_' + itemNumber.toString()).text(deutTotalCost).digits();
                      }
                      else {
                        $('#resDeut_' + itemNumber.toString()).text('bruh pls.')
                      }
                      }
                    });
                  }
                });
          });
        }

      </script>
